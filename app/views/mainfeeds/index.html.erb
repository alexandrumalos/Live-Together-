<p id="notice"><%= notice %></p>

<h1>Listing Mainfeeds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mainfeeds.each do |mainfeed| %>
      <tr>
        <td><%= link_to 'Show', mainfeed %></td>
        <td><%= link_to 'Edit', edit_mainfeed_path(mainfeed) %></td>
        <td><%= link_to 'Destroy', mainfeed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mainfeed', new_mainfeed_path %>
